{
  "plugins": ["plugins/markdown", "plugins/summarize"],
  "recurseDepth": 10,
  "source": {
    "include": ["."],
    "includePattern": ".js$",
    "exclude": ["out", "coverage", "node_modules"]
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./out/",
    "recurse": true
  }
}
